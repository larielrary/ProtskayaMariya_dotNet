@model IEnumerable<BusinessLayer.Models.Car>

<table class="table">
    <thead>
        <tr>
            <th>
                <label asp-display-for="@Html.DisplayNameFor(model => model.CarNumber)"></label>
            </th>
            <th>
                <label asp-display-for="@Html.DisplayNameFor(model => model.CarModel)"></label>
            </th>
            <th>
                <label asp-display-for="@Html.DisplayNameFor(model => model.EngineCapacity)"></label>
            </th>
            <th>
                <label asp-display-for="@Html.DisplayNameFor(model => model.BodyNubmer)"></label>
            </th>
            <th>
                <label asp-display-for="@Html.DisplayNameFor(model => model.YearOfProduction)"></label>
            </th>
            <th>
                <label asp-display-for="@Html.DisplayNameFor(model => model.OwnerId)"></label>
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
        <tr>
            <td asp-display-for="@item.CarNumber"></td>
            <td asp-display-for="@item.CarModel"></td>
            <td asp-display-for="@item.EngineCapacity"></td>
            <td asp-display-for="@item.BodyNubmer"></td>
            <td asp-display-for="@item.YearOfProduction"></td>
            <td asp-display-for="@item.OwnerId"></td>
           <td>
               <a asp-action="Edit" asp-route-id="@item.Id">Edit</a> |
               <a asp-action="Delete" asp-route-id="@item.Id">Delete</a>
            </td>
            
        </tr>
        }
    </tbody>
</table>
<p>
    <a asp-action="Create">Create</a>
</p>
